Integer : isPrime Integer n
    (n < 2) if
        -> 0
    (n == 2) if
        -> 1
    (n % 2 == 0) if
        -> 0
    upperLimit = 0;
    #  upperLimit = sqrt(n);  #
    
    i = 3
    i <= upperLimit while
        (n % i == 0) if
            -> 0
        i += 2
    -> 1


i for 1, 2000000
